###########
# ALIASES #
###########

# Bash command overwrites
alias cat=bat
alias catc='bat -p --paging=never'
alias ls='lsd --icon never'
alias vim=nvim

# Shortenings of random commands
alias copy=clipcopy
alias fetch="rsync -r -e 'ssh -i ~/.ssh/syndis_rsa'"
alias paste=clippaste
alias zt=zerotier-cli
alias docked='autorandr -l docked'

# Tmux aliases
alias tml='tmux ls'
alias tma='tmux attach -t'
alias tmk='tmux kill-session -t'
